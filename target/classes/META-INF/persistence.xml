<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">

  <persistence-unit name = "DONJDBC_PersistenceUnit" transaction-type="JTA">
   <jta-data-source>jdbc/DONJDBC</jta-data-source>
      <class>org.example.data.entity.EUser</class>
      <class>org.example.data.entity.EEvent</class>
      <class>org.example.data.entity.EEventQuestions</class>
      <class>org.example.data.entity.EEventResults</class>
      <class>org.example.data.entity.EAdditionalMaterials</class>
  </persistence-unit>

</persistence>
